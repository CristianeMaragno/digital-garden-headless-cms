{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "customField",
      "options": {
        "preset": "toolbarBaloon"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "name": {
      "type": "string"
    },
    "date": {
      "type": "date"
    },
    "tag": {
      "type": "enumeration",
      "enum": [
        "project",
        "reading",
        "note"
      ]
    },
    "home": {
      "type": "boolean"
    },
    "coverImage": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "completed": {
      "type": "boolean"
    },
    "language": {
      "type": "enumeration",
      "enum": [
        "pt",
        "eng"
      ]
    }
  }
}
